  "paths": {
    "/weatherforecast": {
      "get": {
        "tags": [
          "GettingStarted"
        ],
        "operationId": "GetWeatherForecast",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/weatherforecast/{id}": {
      "get": {
        "tags": [
          "GettingStarted"
        ],
        "operationId": "GetWeatherForecastDay",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WeatherForecast"
                }
              }
            }
          }
        }
      }
    },
    "/weatherforecast/bulk": {
      "get": {
        "tags": [
          "GettingStarted"
        ],
        "operationId": "GetWeatherForecastBulk",
        "parameters": [
          {
            "name": "ids",
            "in": "query",
            "required": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "ids1",
            "in": "query",
            "required": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WeatherForecast"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Demonstrates usage of JsonSqid attribute usage on plain integer properties"
          },
          "ids": {
            "type": "array",
            "items": {
              "pattern": "^-?(?:0|[1-9]\\d*)$",
              "type": "string"
            },
            "description": "Demonstrates usage of JsonSqid attribute usage on arrays"
          },
          "idReference": {
            "type": "string",
            "description": "Use this property to compare with Open API JSON output of int WeatherForecast.Id"
          },
          "idsReference": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Use this property to compare with Open API JSON output of int[] WeatherForecast.Ids"
          },
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperatureC": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "summary": {
            "type": [
              "null",
              "string"
            ]
          },
          "temperatureF": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "GettingStarted"
    }
  ]
}